{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ReadSharedAgentcoreParams",
            "Effect": "Allow",
            "Action": [
                "ssm:GetParameter",
                "ssm:GetParameters"
            ],
            "Resource": "arn:aws:ssm:us-east-2:223382717293:parameter/agentcore/shared/*"
        },
        {
            "Sid": "DecryptSecureString",
            "Effect": "Allow",
            "Action": [
                "kms:Decrypt"
            ],
            "Resource": "*"
        }
    ]
}